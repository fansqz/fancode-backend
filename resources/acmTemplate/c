#include <stdio.h>           //1
int main() {                              //2
    int a, b;                             //3
    a = 1;                                //4
	printf("aaa");                        //5
	scanf("%d", &a);                      //6
    printf("a * a = %d\n", a * a);        //7
	b = 3;                                //8
	scanf("%d", &a);                      //9
    printf("a + a = %d\n", a + a);        //10
    return 0;                             //11
}